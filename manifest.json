{
  "manifest_version": 3,
  "name": "Cursor Checkout Generator",
  "version": "1.0",
  "description": "Generate credit cards and addresses for Cursor Pro checkout",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://checkout.stripe.com/*"],
      "js": ["generator.js", "content.js"]
    }
  ]
}
